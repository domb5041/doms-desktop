ndleSortBy('size')}>\n                    Size\n                    {returnSortSymbol('size')}\n                    <div className='column-resize' />\n                </StyledHeadCol>\n                <StyledHeadCol onClick={() => handleSortBy('type')}>\n                    Kind\n                    {returnSortSymbol('type')}\n                </StyledHeadCol>\n            </StyledListHeader>\n            <StyledFiles>\n                {files2.map((file, i) => (\n                    <>\n                        <StyledFile\n                            evenRow={i % 2 === 0}\n                            onClick={() => setActiveFile(file.id)}\n                            onDoubleClick={() =>\n                                handleOpenMessageBox(file.name)\n                            }\n                            activeFile={file.id === activeFile}\n                        >\n                            <div>{file.name}</div>\n                            <div>\n                                {formatDate(\n                                    file.dateModified,\n                                    'dd mmm yyyy at hh:mm'\n                                )}\n                            </div>\n                            <div>{formatBytes(file.size)}</div>\n                            <div>{file.type}</div>\n                        </StyledFile>\n                    </>\n                ))}\n            </StyledFiles>\n        </StyledListView>\n    );\n}\n",["96","97"],"/Users/dominicbutler/Desktop/retro-finder/src/data/fileStructure.js",["98"],"export default [\n    {\n        id: '1',\n        name: 'Downloads',\n        files: [\n            {\n                id: '820398420',\n                name: 'download 1',\n                dateModified: '1610625843',\n                size: 39749875,\n                type: 'JPEG Image',\n            },\n            {\n                id: '2131288',\n                name: 'moo',\n                dateModified: '1610895843',\n      