   text-overflow: ellipsis;\n        margin-right: 7px;\n    }\n`;\n\nexport default function ListView({ files, openMessageBox, setMessageBoxText }) {\n    const [activeFile, setActiveFile] = useState(null);\n    const [files2, setFiles2] = useState(files);\n    const [sortBy, setSortBy] = useState([null, false]);\n\n    useEffect(() => handleSortBy('name'), []);\n\n    const handleSortBy = col => {\n        const files2Copy = [...files2];\n        const reverse = (sortBy[0] === col && !sortBy[1]) || false;\n\n        const sortText = (a, b) =>\n            a[col] === b[col] ? 0 : a[col] < b[col] ? -1 : 1;\n        const sortTextR = (a, b) =>\n            a[col] === b[col] ? 0 : b[col] < a[col] ? -1 : 1;\n        const sortNumber = (a, b) => a[col] - b[col];\n        const sortNumberR = (a, b) => b[col] - a[col];\n\n        const sortFn = () => {\n            switch (col) {\n                case 'name':\n                case 'type':\n                    return reverse ? sortTextR : sortText;\n                case 'size':\n                case 'dateModified':\n                    return reverse ? sortNumberR : sortNumber;\n            }\n        };\n\n        setSortBy([col, reverse]);\n        setFiles2(files2Copy.sort(sortFn()));\n    };\n\n    const returnSortSymbol = col => {\n        if (sortBy[0] === col) {\n            return <div className='sort-arrow'>{sortBy[1] ? '⋀' : '⋁'}</div>;\n        }\n        return null;\n    };\n\n    const handleOpenMessageBox = fileName => {\n        const text = fileName + \" couldn't be found\";\n        openMessageBox();\n        setMessageBoxText(text);\n    };\n\n    return (\n        <StyledListView>\n            <StyledListHeader>\n                <StyledHeadCol onClick={() => handleSortBy('name')}>\n                    Name\n                    {returnSortSymbol('name')}\n                    <div className='column-resize' />\n                </StyledHeadCol>\n                <StyledHeadCol onClick={() => handleSortBy('dateModified')}>\n                    Date Modified\n                    {returnSortSymbol('dateModified')}\n                    <div className='column-resize' />\n                </StyledHeadCol>\n                <StyledHeadCol onClick={() => handleSortBy('size')}>\n                    Size\n                    {returnSortSymbol('size')}\n                    <div className='column-resize' />\n                </StyledHeadCol>\n                <StyledHeadCol onClick={() => handleSortBy('type')}>\n                    Kind\n                    {returnSortSymbol('type')}\n                </StyledHeadCol>\n            </StyledListHeader>\n            <StyledFiles>\n                {files2.map((file, i) => (\n                    <>\n                        <StyledFile\n                            evenRow={i % 2 === 0}\n                            onClick={() => setActiveFile(file.id)}\n                            onDoubleClick={() =>\n                                handleOpenMessageBox(file.name)\n                            }\n                            activeFile={file.id === activeFile}\n                        >\n                            <div>{file.name}</div>\n                            <div>\n                                {formatDate(\n                                    file.dateModified,\n                                    'dd mmm yyyy at hh:mm'\n                                )}\n                            </div>\n                            <div>{formatBytes(file.size)}</div>\n                            <div>{file.type}</div>\n                        </StyledFile>\n                    </>\n                ))}\n            </StyledFiles>\n        </StyledListView>\n    );\n}\n",["102","103"],"/Users/dominicbutler/Desktop/retro-finder/src/data/fileStructure.js",["104"],"/Users/dominicbutler/Desktop/retro-finder/src/dock/Dock.js",[],"/Users/dominicbutler/Desktop/retro-finder/src/data/menuItems.js",["105"],"export default [\n    {\n        id: 'asd7-f0a98s